{
  "expo": {
    "name": "Breed Dog Finder",
    "slug": "breed-dog-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "breeddogfinder",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon-splash.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#753E05"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaayalalafise.breeddogfinder",
      "appleTeamId": "ZJAD2N25T4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-transparent.png",
        "backgroundColor": "#753E05"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jaayalalafise.breeddogfinder",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#753E05"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) para acceder a la camara",
          "microphonePermission": "Permitir $(PRODUCT_NAME) para acceder al micr√≥fono",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir $(PRODUCT_NAME) para acceder a Fotos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "25788332-fe92-43c1-8617-d1461dece157"
      }
    },
    "owner": "ayalabuitrago"
  }
}
